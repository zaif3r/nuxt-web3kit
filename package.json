{
  "name": "@zaifer/nuxt-web3kit",
  "version": "1.0.4",
  "author": "zaifer",
  "description": "Nuxt Web3Kit module: web+ composables",
  "repository": {
    "type": "git",
    "url": "https://github.com/zaif3r/nuxt-web3kit.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run unit && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@ethersproject/abstract-provider": "^5.7.0",
    "@nuxt/kit": "^3.1.1",
    "@pinia-plugin-persistedstate/nuxt": "^1.0.0",
    "@pinia/nuxt": "^0.4.6",
    "@vueuse/core": "^9.12.0",
    "@wagmi/core": "^0.9.2",
    "@zaifer/nuxt-query": "^1.0.2",
    "defu": "^6.1.2",
    "ethers": "^5.7.2",
    "jsonwebtoken": "^9.0.0",
    "web3.storage": "^4.4.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.1.1",
    "@nuxt/test-utils": "^3.1.1",
    "@types/jsonwebtoken": "^9.0.1",
    "changelogen": "^0.4.1",
    "eslint": "^8.33.0",
    "nuxt": "^3.1.1",
    "vitest": "^0.28.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
